{# vim:se syntax=html.tornadotmpl: #}
{% autoescape None %}

{% for x in root["items"] %}
{ item_base, {{", ".join(repr(x[k]) for k in ["id","name","desc","icon","star","cls","price","attributes","skills"]) }} }.
{% end %}

{% for x in root.heroes %}
{hero_base, { {{x.id}}, {{x.lv}} },{{",".join(repr(x[k]) for k in ["id","lv","exp","star","name","icon","asset","speed","att_speed","att_radius","skills","hp","mp","re_hp","re_mp","re_hp2","att","arm","ma","md","ar","dod","da","dad","dar","dadr"])}}}.
{% end %}

{% for i in range(1, 6) %}
{% for x in root["equip_levelup_class" + str(1)] %}
{% for star in range(x.star, 6) %}
{ equipment_levelup, { {{ i }}, {{ star }}, {{ x.lv }} },  {{", ".join(repr(x[k]) for k in ["lv","star","exp","attributes"]) }} }.
{% end %}
{% end %}
{% end %}
